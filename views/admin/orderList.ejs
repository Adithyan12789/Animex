    <main class="main-wrap">
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Order Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Details</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% orders.forEach(order => { %>
                                <tr>
                                    <td>ord_<%= order.trackingId %></td>
                                    
                                    <td>
                                        <% console.log(order); %>
                                        <% order.items.forEach(item => { %>
                                            <% if (item.product && item.product.name) { %>
                                                <b><%= item.product.name %></b>, <br>
                                            <% } else { %>
                                                <i>No product name available</i>, <br>
                                            <% } %>
                                        <% }) %>
                                    </td>
                                    
                                    <td>₹ <%= order.totalPrice %></td>
                                    <td><span class="badge rounded-pill alert-warning">
                                        <% if (order.paymentStatus) { %>
                                            <%= order.paymentStatus %>
                                        <% }else{ %>
                                            <p>there is no payment Status</p>
                                        <% } %>
                                        </span></td>
                                    <td><span class="badge rounded-pill alert-warning">
                                    <% if (order.orderStatus) { %>
                                        <%= order.orderStatus %>
                                    <% }else{ %>
                                        <p>there is no Order Status</p>
                                    <% } %>
                                    </span></td>
                                    <td><%= new Date(order.orderDate).toLocaleDateString("en-US", { year: 'numeric', month: 'long', day: 'numeric' }) %></td>
                                    <td><a href="/orderDetails/<%= order._id %>" class="btn btn-md rounded font-sm">Detail</a></td>
                                    <td>
                                        <button class="btn btn-sm btn-primary" onclick="deleteOrder('<%= order._id %>')">Cancel Order</button>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                        </table>                            
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->  

            <div class="container text-center"> <!-- Centered container div -->
                <div class="paginationCenter mt-30 mb-50">
                    <nav aria-label="Page navigation example">
                        <ul class="pagination justify-content-start">
                            <!-- Left arrow icon -->
                            <li class="page-item">
                                <a class="page-link" href="/orderList?page=<%= currentPage - 1 %>" aria-label="Previous">
                                    <span aria-hidden="true">&laquo;</span>
                                </a>
                            </li>
                            <% for(i = 1 ; i <= totalPages ; i++){ %>
                                <li class="page-item <%= currentPage == i ? "active": "" %>">
                                    <a class="page-link" href="/orderList?page=<%= i %>"><%= i %></a>
                                </li>
                            <% } %>
                            <!-- Right arrow icon -->
                            <li class="page-item">
                                <a class="page-link" href="/orderList?page=<%= currentPage + 1 %>" aria-label="Next">
                                    <span aria-hidden="true">&raquo;</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </section> <!-- content-main end// -->
